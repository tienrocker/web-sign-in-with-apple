    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "firebase/php-jwt": "^5.0",
        "lcobucci/jwt": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Apple\\": "src"
        },
        "classmap": [
            {
                "Firebase\\JWT\\JWK": "src/JWK.php"
            }
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    }
}

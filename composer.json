{
    "name": "tienrocker/web-sign-in-with-apple",
    "description": "Sample php web sign in with apple",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "tien tran",
            "email": "tienrocker@gmail.com",
            "role": "Developer"
        }
